    "    \"            else:\\n\",\n",
    "    \"                edge_dict[author_pair] = 1\\n\",\n",
    "    \"df = pd.DataFrame([{\\\"author_1\\\": pair[0], \\\"author_2\\\": pair[1], \\\"weight\\\": weight} for pair, weight in edge_dict.items()])\\n\",\n",
    "    \"df\"\n",
